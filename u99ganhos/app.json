{
  "expo": {
    "name": "u99ganhos-app",
    "slug": "u99ganhos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "u99ganhosapp",
    "userInterfaceStyle": "automatic",
    "sdkVersion": "54.0.0",
    "newArchEnabled": true, 
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "O app precisa acessar sua localização em segundo plano para rastrear seus percursos de trabalho.",
        "NSLocationAlwaysUsageDescription": "O app precisa acessar sua localização em segundo plano para rastrear seus percursos de trabalho.",
        "NSLocationWhenInUseUsageDescription": "O app precisa acessar sua localização para rastrear seus percursos de trabalho."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.lzin9889.u99ganhosapp",
      "gradleProperties": [
        {
          "type": "property",
          "key": "newArchEnabled",
          "value": "true"
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-asset",
      "expo-audio",
      "expo-font",
      "expo-localization",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true,
          "androidForegroundServiceNotificationTitle": "Sessão de Trabalho Ativa",
          "androidForegroundServiceNotificationSubTitle": "Rastreando seu percurso em segundo plano",
          "androidForegroundServiceNotificationIcon": "./assets/images/logo.png",
          "androidForegroundServiceNotificationColor": "#00A85A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eac35f1c-1112-4817-a25c-de1f2587935a"
      }
    },
    "owner": "lzin9889"
  }
}